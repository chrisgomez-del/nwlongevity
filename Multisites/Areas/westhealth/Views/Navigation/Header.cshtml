    @*Mobile Menu*@
    <div class="offcanvas-body">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0 gap-3">
            @foreach (var navItem in Model.Links)
            {
                var linkClass = navItem.IsActive || (navItem.Children != null && navItem.Children.Any(kid => kid.IsActive)) ? "active" : string.Empty;
                var index = 0;
                if (navItem.Children != null && navItem.Children.Any())
                {
                    <li class="nav-item dropdown d-flex flex-column align-items-between">
                        <div class="d-flex justify-content-between">
                            <a class="nav-link btn btn-link @linkClass" href="@navItem.LinkSource">
                                @navItem.LinkText
                            </a>
                            <button class="btn btn-link"
                                    type="button"
                                    data-bs-toggle="collapse"
                                    data-bs-target="#mobileMenuCollapse-@index"
                                    aria-expanded="false"
                                    aria-controls="mobileMenuCollapse-@index">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708" />
                                </svg>
                                <span class="visually-hidden">Toggle Submenu</span>
                            </button>
                        </div>
                        <ul class="collapse w-100" id="mobileMenuCollapse-@index">
                            @foreach (var childItem in navItem.Children)
                            {
                                <li><a class="dropdown-item" href="@childItem.LinkSource">@childItem.LinkText</a></li>
                            }
                        </ul>
                    </li>
                    index++;
                }
                else
                {
                    <li class="nav-item">
                        <a class="nav-link" href="@navItem.LinkSource">@navItem.LinkText</a>
                    </li>
                    index++;
                }
            }
        </ul>
        <ul class="navbar-nav navbar-nav-utility mb-2 mb-lg-0">
            <li class="nav-item">
                <a class="nav-link" aria-current="page" href="#">Contact <i class="bi bi-envelope"></i></a>
            </li>
        </ul>
    </div>
</div>